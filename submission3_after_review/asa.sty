\long\def\@makecaption#1#2{
 \vskip 10pt 
 \setbox\@tempboxa\hbox{\em #1. #2}      % was : not .
 \ifdim \wd\@tempboxa >\hsize{\em #1. #2}\par \else 
        \hbox to\hsize{\hfil\box\@tempboxa\hfil} 
 \fi}

